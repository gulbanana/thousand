class inverse:object [fill=black] {
    class line [stroke=white]
    class object:object [stroke=white,font=white]
}

diagram.column {
    group [gutter=10] {
        object a
        object b 
        line a--b
    }

    group.inverse [gutter=10] {
        object a
        object b
        line a--b
    }
}