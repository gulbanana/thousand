scale = 2.0
gutter = 40

class node [fill=#ccc,shape=circle,stroke=1]
class hair [stroke=hairline]
class scale [stroke=1]
class thin : node.hair
class dash [stroke=dashed]

thin A 
thin.dash B
node.dash C
thin D [row=2]
node E 
node F

hair.dash a-> b
hair a-> e
hair a-> d

scale.dash b-> c 
scale b-> e 
scale b-> f 